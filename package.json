{
  "name": "browse-it",
  "version": "0.0.3",
  "displayName": "Browse It from VS Code",
  "description": "VS Code extension for browsing files.",
  "publisher": "leadzen",
  "categories": ["Other"],
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/leadzen/browseit"
  },
  "engines": { "vscode": "^0.10.0" },
  "main": "main.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {"command": "browse.it", "title": "Browser It", "icon": { "dark": "./icon.png", "light": "./icon.png" }}
    ],
    "menus": {
      "editor/context": [{"command": "browse.it"}],
      "editor/title": [{"command": "browse.it"}],
      "editor/title/context": [{"command": "browse.it"}],
      "explorer/context": [{"command": "browse.it"}]
    }
  }
}